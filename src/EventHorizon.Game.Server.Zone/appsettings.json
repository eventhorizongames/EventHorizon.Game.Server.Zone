{
    "OwnerDetails": {
        "UserId": "",
        "PlatformId": ""
    },
    "EnableTestingMode": false,
    "ServerName": "Zone",
    "Auth": {
        "Authority": "",
        "ApiName": "Api.Core",
        "ClientId": "",
        "ClientSecret": ""
    },
    "Core": {
        "Server": ""
    },
    "Player": {
        "Server": ""
    },
    "Agent": {
        "Server": ""
    },
    "Cors": {
        "Hosts": []
    },
    "Monitoring": {
        "ApplicationInsights": {
            "Enabled": false,
            "InstrumentationKey": ""
        }
    },
    "Reporter": {
        "IsEnabled": false,
        "IsWriteToFileEnabled": false,
        "Elasticsearch": {
            "IsEnabled": false,
            "Url": "http://localhost:9200"
        }
    },
    "MetricsOptions": {
        "DefaultContextLabel": "MyMvcApplication",
        "Enabled": true
    },
    "Systems": {
        "ClientScripts": {
            "CompilerSubProcessDirectory": "/sub-processes/client-scripts",
            "CompilerSubProcess": "EventHorizon.Game.Server.Zone.Client.Scripts.SubProcess"
        },
        "ServerScripts": {
            "CompilerSubProcessDirectory": "/sub-processes/server-scripts",
            "CompilerSubProcess": "EventHorizon.Game.Server.Zone.Server.Scripts.SubProcess"
        }
    },
    "Plugins": {
        "ClientScriptsPluginCompiler": {
            "SdkPackage": "EventHorizon.Game.Client.Scripts.SDK",
            "SdkPackageVersion": "*",
            "IncludePrerelease": false,
            "NuGetFeed": "https://api.nuget.org/v3/index.json"
        }
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "IdentityServer4.AccessTokenValidation": "Information",
                "EventHorizon.Zone.System.Server.Scripts.Run": "Information",
                "EventHorizon.Server.Core.Ping": "Warning"
            }
        },
        "Enrich": [
            "WithThreadId"
        ],
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Timestamp:MM-dd-yyyy HH:mm:ss.fffffffzzz}] [{ProcessIdentifier}] [{PlatformId}] [{Host}] [{EnvironmentName}] [{ServiceName}] [{SourceContext}] [{ThreadId}] [{Level}] {Message}{NewLine}{Exception}"
                }
            },
            {
                "Name": "Elasticsearch",
                "Args": {
                    "nodeUris": "http://localhost:9200",
                    "autoRegisterTemplate": true
                }
            }
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
}
